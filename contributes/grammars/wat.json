          "begin": "@",
          "beginCaptures": {
            "0": {
              "name": "meta.annotation.name.wasm punctuation.definition.tag"
            }
          },
          "end": "(?=\\))",
          "endCaptures": {
            "0": {
              "name": "meta.brace.round.annotation.wasm"
            }
          },
          "patterns": [
            {
              "begin": "[0-9A-Za-z!#$%&'*+-./:<=>?@\\^_'|~]+",
              "beginCaptures": {
